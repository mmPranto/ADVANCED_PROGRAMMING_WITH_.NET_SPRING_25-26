
<table class="table table-bordered">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Age</th>
            <th>Department</th>
            <th>CGPA</th>
            <th>Minor/Adult</th>
            <th>Excellent/Average</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
                <td>@item.Age</td>
                <td>@item.Department</td>
                <td>@item.Cgpa</td>
                @if (item.Age >= 20)
                {
                    <td>Adult</td>
                }
                else
                {
                    <td>Minor</td>
                }
                @if (item.Cgpa >= 3.5)
                {
                    <td>Excellent</td>
                }
                else
                {
                    <td>Average</td>
                }
            </tr>   
        }
    </tbody>
</table>